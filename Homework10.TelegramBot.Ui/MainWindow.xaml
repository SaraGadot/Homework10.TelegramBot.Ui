<Window x:Class="Homework10.TelegramBot.Ui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework10.TelegramBot.Ui"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid >
        <ListBox x:Name="Users_List" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,526,0"/>
        <TextBox x:Name="Message_TextBox" HorizontalAlignment="Left" Margin="308,66,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="282" Height="108"/>
        <Button x:Name="Send_Button" Content="Send" HorizontalAlignment="Left" Margin="539,195,0,0" VerticalAlignment="Top" Click="Send_Button_Click"/>

    </Grid>
</Window>
